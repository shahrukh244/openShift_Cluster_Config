zone "kube.lan" {
    type master;
    file "/etc/bind/zones/db.kube.lan";

    notify yes;
    allow-transfer { 10.0.0.11; 10.0.0.12; };
    also-notify  { 10.0.0.11; 10.0.0.12; };
};

zone "0.0.10.in-addr.arpa" {
    type master;
    file "/etc/bind/zones/db.reverse";

    notify yes;
    allow-transfer { 10.0.0.11; 10.0.0.12; };
    also-notify  { 10.0.0.11; 10.0.0.12; };
};
